{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter-wonders-app",
            "request": "launch",
            "type": "dart",
            "args": [
                "--local-engine",
                "android_debug_unopt_arm64",
                "--local-engine-host",
                "host_debug_unopt_arm64",
                "--enable-impeller",
            ]
        },
        {
            "name": "flutter-wonders-app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter-wonders-app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}
